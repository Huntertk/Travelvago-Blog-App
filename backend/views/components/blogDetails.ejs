<div class="blog_detials_container">
    <h1><%=blog.title%></h1>
    <hr>
    <span><%=blog.createdAt.toLocaleDateString('en-US', { year: 'numeric', month: 'long', day: 'numeric' })%></span>
    <div class="blog_details_img_container">
        <img src="<%=blog.image%>" alt="<%=blog.title%>">
    </div>
    <div class="blog_details_content_container">
        <%-blog.content%>
    </div>
</div>